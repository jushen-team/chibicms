@using Jushen.ChibiCms.ChibiContent
@model Content

@{
    ViewData["Title"] = Model.Meta.Title;
    Layout = "_Layout";
    var photos = Newtonsoft.Json.JsonConvert.DeserializeObject<List<string>>(Model.Meta.Extras["photos"].ToString());
}


<div class="row pt-4 mb-5 text-center">
    <div class="col-12">
        <h2 class="text-white mb-4">@Model.Meta.Title &mdash; @photos.Count Photos</h2>
    </div>
</div>

@Html.Partial("photoPartial", photos);





